export O2_PATH="$HOME/Projects/openobserve"
export O2_ENT_PATH="$HOME/Projects/o2-enterprise"

# Function to handle enterprise cargo run with arguments
function run_enterprise() {
    if [ -f "${O2_ENT_PATH}/Cargo.toml.openobserve" ]; then
      cp "${O2_ENT_PATH}/Cargo.toml.openobserve" "$(pwd)/Cargo.toml"
      ZO_META_POSTGRES_DSN="postgres://postgres:password@localhost:5432/openobserve" cargo run --features enterprise -- -c "$O2_PATH/.env" "$@" 
    else
        echo "Error: Cargo.toml.openobserve not found in ${O2_ENT_PATH}"
        return 1
    fi
}

# Function to handle enterprise cargo run with arguments
function run_cloud() {
    if [ -f "${O2_ENT_PATH}/Cargo.toml.openobserve" ]; then
      cp "${O2_ENT_PATH}/Cargo.toml.openobserve" "$(pwd)/Cargo.toml"
      ZO_META_POSTGRES_DSN="postgres://postgres:password@localhost:5432/openobserve_cloud" cargo run --features cloud -- -c "$O2_PATH/.env.cloud" "$@" 
    else
        echo "Error: Cargo.toml.openobserve not found in ${O2_ENT_PATH}"
        return 1
    fi
}


# Replace the existing alias with the new function
alias cre="run_enterprise"
