# Function to run ingestion benchmark with configurable connection parameters
function ingest_data() {
  # Execute the benchmark
  pushd ~/projects/deployments/benchmark-o2
  trap 'popd' INT  # Return to previous directory when Ctrl+C is pressed
  go run profile_index_stream.go "$@"
  popd
}

# Function to list PRs that need your review in both repos
function prr() {
  echo "=== OpenObserve PRs to Review ==="
  (cd ~/Projects/openobserve && GH_PAGER="" gh pr list --search "is:open review-requested:@me" --json number,title,url,author --template '{{range .}}#{{.number}} - {{.title}} (by @{{.author.login}})
  {{.url}}

{{end}}')
  echo ""
  echo "=== O2 Enterprise PRs to Review ==="
  (cd ~/Projects/o2-enterprise && GH_PAGER="" gh pr list --search "is:open review-requested:@me" --json number,title,url,author --template '{{range .}}#{{.number}} - {{.title}} (by @{{.author.login}})
  {{.url}}

{{end}}')
}

