# Function to handle enterprise cargo run with arguments
function run_enterprise() {
  if [ -f "../o2-enterprise/Cargo.toml.openobserve" ]; then
    cp "../o2-enterprise/Cargo.toml.openobserve" "$(pwd)/Cargo.toml"

    # Start frontend in background
    echo "Starting frontend..."
    (cd web && [ ! -d "node_modules" ] && npm install; npm run dev) &
    local FE_PID=$!

    # Trap to kill frontend when function exits
    trap "echo 'Stopping frontend...'; kill $FE_PID 2>/dev/null; wait $FE_PID 2>/dev/null" EXIT INT TERM

    # Run backend in foreground
    cargo run --features enterprise -- -c ".env" "$@"

    # Cleanup trap
    trap - EXIT INT TERM
    kill $FE_PID 2>/dev/null
    wait $FE_PID 2>/dev/null
  else
    echo "Error: Cargo.toml.openobserve not found in ../o2-enterprise"
    return 1
  fi
}

# Function to handle cloud cargo run with arguments
function run_cloud() {
  if [ -f "../o2-enterprise/Cargo.toml.openobserve" ]; then
    cp "../o2-enterprise/Cargo.toml.openobserve" "$(pwd)/Cargo.toml"

    # Start frontend in background
    echo "Starting frontend..."
    (cd web && [ ! -d "node_modules" ] && npm install; npm run dev) &
    local FE_PID=$!

    # Trap to kill frontend when function exits
    trap "echo 'Stopping frontend...'; kill $FE_PID 2>/dev/null; wait $FE_PID 2>/dev/null" EXIT INT TERM

    # Run backend in foreground
    cargo run --features cloud -- -c ".env.cloud" "$@"

    # Cleanup trap
    trap - EXIT INT TERM
    kill $FE_PID 2>/dev/null
    wait $FE_PID 2>/dev/null
  else
    echo "Error: Cargo.toml.openobserve not found in ../o2-enterprise"
    return 1
  fi
}


