# Function to run ingestion benchmark with configurable connection parameters
function ingest_data() {
    # Execute the benchmark
    pushd ~/tools/benchmark-o2
    trap 'popd' INT  # Return to previous directory when Ctrl+C is pressed
    
    go run profile_index_stream.go "$@"
    
    popd
}

